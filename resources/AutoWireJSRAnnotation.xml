<?xml version="1.0" encoding="UTF-8"?>

<beans  xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:context="http://www.springframework.org/schema/context"
		xsi:schemaLocation="http://www.springframework.org/schema/beans 
    						http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
   							http://www.springframework.org/schema/context 
    						http://www.springframework.org/schema/context/spring-context-4.0.xsd">
	
	<context:annotation-config/>
	
	<bean class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"/>
	<bean id="engine" class="spring.di.autowire.jsrannotation.Engine">
		<property name="modelYear" value="2016"/>
	</bean>
	
	<bean id="e2" class="spring.di.autowire.jsrannotation.Engine">
		<property name="modelYear" value="2015"/>
	</bean>
	
	<bean id="car_annotation" class="spring.di.autowire.jsrannotation.Car">
		<constructor-arg value="Ferrari"/>
	</bean>
	
</beans>