<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="engine" class="spring.di.autowire.name.Engine">
		<property name="modelYear" value="2016"/>
	</bean>
	
	<bean id="e2" class="spring.di.autowire.name.Engine" >
		<property name="modelYear" value="2015"/>
	</bean>
	
	<bean id="car_byname" class="spring.di.autowire.name.Car" autowire="byName">
		<property name="carName" value="Ferrari"></property>
	</bean>
	
</beans>